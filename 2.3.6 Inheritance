import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
// https://github.com/OpenZeppelin/openzeppelin-contracts/blob/master/contracts/token/ERC20/ERC20.sol


//Task 2.3.6 Inheritance
//StudentDiscordID = BobZedF#2556

contract YourContract is ERC20 {

//Create an ERC-20 called Testy with symbol TST
//Mint an initial supply of 10 000 (adjusted for default decimals) and send to msg.sender

 constructor() ERC20("Testy", "TST") {
        uint256 initialSupply = 10000 * (10 ** decimals());
        _mint(msg.sender, initialSupply);
    }
}
